{
  "include": [
    "*.py"
  ],
  "exclude": [
    "**/.git",
    "**/__pycache__",
    "**/node_modules", 
    "**/.venv/**",
    "**/venv/**",
    "**/htmlcov",
    "tests/**",
    "setup/**",
    "scripts/**", 
    "improvements/**",
    "comprehensive_test_fix.py",
    "fix_remaining_tests.py"
  ],
  "venvPath": ".",
  "venv": ".venv",
  "autoSearchPaths": false,
  "extraPaths": [],
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": "information",
  "pythonVersion": "3.13", 
  "pythonPlatform": "All",
  "typeCheckingMode": "basic",
  "pythonPath": ".venv/bin/python"
}