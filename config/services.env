# PR Review Services Configuration

# GitHub Configuration (required)
GITHUB_TOKEN=your_github_token_here
# Note: Repository is auto-detected from git remote origin

# CI/CD Configuration (optional)
CI_API_KEY=your_ci_api_key_here

# Server Configuration  
SERVER_HOST=0.0.0.0
SERVER_PORT=8080
WORKER_HOST=0.0.0.0  
WORKER_PORT=8081
WORKERS=1

# Worker Configuration
POLL_INTERVAL=30
AUTO_START_WORKER=true

# Database Configuration
DATABASE_URL=sqlite:///~/.local/share/github-agent/pr_replies.db

# Logging
LOG_LEVEL=INFO
